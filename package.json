{
  "type": "module",
  "name": "vs-mcp",
  "displayName": "AI Extension",
  "description": "A minimal VSCode extension built with Vite and Vitest",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vs-mcp.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-mcp.runAgent",
        "title": "Run MCP Agent"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vs-mcp.runAgent",
          "when": "resourceFilename =~ /\\.agent\\.js$/",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "@vitest/ui": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "openai": "^5.20.1",
    "vscode": "^1.1.37"
  }
}
